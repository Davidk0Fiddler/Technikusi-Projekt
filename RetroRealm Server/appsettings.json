{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.log",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true
        }
      }
    ]
  },
  "Jwt": {
    "Key": "9FqL8xZrA!m4K#D2vP@YcW7H$EJtS5B0",
    "Issuer": "MyApp",
    "Audience": "MyAppUsers",
    "ExpireMinutes": 30,
    "RefreshExpireMinutes": 600
  },
  "ConnectionStrings": {
    "RetroRealmDatabaseContex": "Data Source=SQLiteRetroRealm.db",
    "RetroRealmLogDatabaseContex": "Data Source=SQLiteRetroRealmLog.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
